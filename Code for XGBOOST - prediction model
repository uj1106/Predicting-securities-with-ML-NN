        "# print(\"Shape of df after inner join:\", df.shape)\n",
        "#\n",
        "# # # 5. Keep only the columns we need for feature engineering\n",
        "# df = df[['Close', 'High', 'Low', 'Volume', 'VIX', 'Interest_rate', 'USD_INR']]\n",
        "#\n",
        "# print(\"Data loading complete. Sample:\")\n",
        "# print(df.head())\n",
